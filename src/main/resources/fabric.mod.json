{
    "schemaVersion": 1,
    "id": "gimm1q",
    "version": "${version}",
    "name": "Gimm1q",
    "description": "Mim1q's modding library.\nContains various utilities for creating fun and immersive mods.\nThese include: camera screenshake, animation utilities, item model utilities and more!",
    "authors": [
        "Mim1q"
    ],
    "contact": {
        "repo": "https://github.com/mimiq/gimm1q",
        "homepage": "https://github.com/mim1q/gimm1q",
        "issues": "https://github.com/mim1q/gimm1q/issues"
    },
    "license": "MIT",
    "icon": "assets/gimm1q/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "dev.mim1q.gimm1q.client.Gimm1qClient"
        ],
        "main": [
            "dev.mim1q.gimm1q.Gimm1q"
        ]
    },
    "mixins": [
        "gimm1q.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.mim1q.dev",
                "modmenu.donate": "https://ko-fi.com/mim1q",
                "modmenu.source": "https://github.com/mim1q/gimm1q"
            },
            "badges": [ "library" ],
            "update_checker": false
        }
    }
}
